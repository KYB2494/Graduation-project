<launch>


	<node pkg="map_server" type="map_server" name="map_server" args="$(find wecar)/map/wall.yaml"/>

	<include file="$(find wecar)/launch/includes/amcl_diff.launch"/>
	<node pkg="wecar" type="amcl_to_odom.py" name="amcl_to_odom"/>
	<include file="$(find wecar)/launch/includes/follow_trajectory_amcl_stop.launch"/>
	<include file="$(find wecar)/launch/includes/waypoint_control_amcl.launch"/>

	<node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher" />
	<node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" />
	<param name="robot_description" command="$(find xacro)/xacro $(find wecar)/urdf/racecar.xacro"/>

	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find wecar)/rviz/amcl_navigation.rviz"/>

</launch>
